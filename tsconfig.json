{
  "compilerOptions": {
    "outDir": "./assets/js",
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noEmit": false,
    "target": "ES6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
  },
  "exclude": [
    "./*.js",
    "./assets/**/*"
  ],
  "include": [
    "./src/index.ts"
  ]
}